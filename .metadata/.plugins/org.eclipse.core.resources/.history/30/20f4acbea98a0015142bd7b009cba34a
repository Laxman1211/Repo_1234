package Day1;

public class One {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=1;
		while(i<=5)
		{
			System.out.println("I value is "+i);
		i++;
		}
		
		System.out.println("after while I value"+ i);
	}

}





