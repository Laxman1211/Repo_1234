package Day1;

public class Arrays {

	public static void main(String[] args) {
		
		int a=20,b=5,c=15;

		if(a<b & a<c)
		{
			System.out.println("a is the smallest");
		}
		else if(b<c)
		{
			System.out.println("b is the smallest");
		}
		else
		{
			System.out.println("c is the smallest");
		}

	}

}
