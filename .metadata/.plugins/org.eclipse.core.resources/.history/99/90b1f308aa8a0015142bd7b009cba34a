package Day1;

public class One {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=1;

		for( i=0; i<=5; i++)
			{	
		
			System.out.println("I value is "+i);
			
			}	
			
		System.out.println(i);

	}

}





