package Day2;

import java.util.ArrayList;
import java.util.List;

public class List1 {

	public static void main(String[] args) {
		
	List<String> l=new ArrayList<>();
	l.add("abc");
	l.add("bcd");
	l.add("jkl");
	l.add("xyz");
	
	for(int i=0; i<l.size(); i++)
	{
	
		System.out.println(l.get(2));
				
	}
	
	System.out.println(l.size());
	
	}

}
